<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="421.0" prefWidth="638.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="travelagencycode.HomePageFXMLController">
    <children>
      <ImageView fitHeight="658.0" fitWidth="638.0" pickOnBounds="true" preserveRatio="true" AnchorPane.leftAnchor="0.0">
         <image>
            <Image url="@Home%20Image.jpeg" />
         </image>
      </ImageView>
      <Label layoutX="108.0" layoutY="35.0" style="-fx-border-style: solid;" text="Welcome To Our HomePage">
         <font>
            <Font name="Calisto MT Bold" size="32.0" />
         </font>
      </Label>
      <Button fx:id="signup" layoutX="120.0" layoutY="197.0" mnemonicParsing="false" onAction="#SignUpMethod" prefHeight="38.0" prefWidth="117.0" style="-fx-background-color: #5758BB;" text="SignUp" textFill="#e8d914">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Button>
      <Button fx:id="login" layoutX="416.0" layoutY="197.0" mnemonicParsing="false" onAction="#gotoLogIn" prefHeight="38.0" prefWidth="117.0" style="-fx-background-color: #5758BB;" text="Login" textFill="#e8d914">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Button>
      <Button fx:id="adminlogin" layoutX="245.0" layoutY="319.0" mnemonicParsing="false" onAction="#AdminLogin" prefHeight="38.0" prefWidth="134.0" style="-fx-background-color: #5758BB;" text=" Admin Login" textFill="#ffea00c2">
         <font>
            <Font name="System Bold Italic" size="17.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
